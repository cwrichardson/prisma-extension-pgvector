{
	"name": "devtest",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "docker compose up -d && node --watch ./index.js",
		"build": "pnpm run generate && pnpm run docker:build",
		"clean": "docker compose down",
		"docker:build": "docker compose build",
		"docker:stop": "docker compose stop",
		"generate": "pnpx prisma generate"
	},
	"dependencies": {
		"@prisma/client": "^5.7.0",
		"pgvector": "^0.1.8",
		"prisma-extension-pgvector": "workspace:^"
	},
	"devDependencies": {
		"eslint": "^8.57.0",
		"@eslint/js": "^9.2.0",
		"@stylistic/eslint-plugin-js": "^2.1.0",
		"prisma": "^5.7.0"
	},
	"prisma": {
		"seed": "node ./prisma/seed.js"
	}
}